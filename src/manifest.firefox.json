{
  "manifest_version": 2,
  "name": "TimeTable for HOPE",
  "version": "1.1.1",
  "description": "hope.fun.ac.jpに時間割機能を追加します。",
  "content_scripts": [
    {
      "matches": ["*://hope.fun.ac.jp/my/*"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "hope-timetable@cathiecode.miraidai.fun",
      "strict_min_version": "42.0"
    }
  }
}
